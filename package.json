{
  "name": "work-log",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run i18n && ng serve --proxy-config src/proxy.conf.json --open",
    "start:prod": "npm run i18n && ng serve --proxy-config src/proxy.conf.json --open --prod",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.ts\"",
    "e2e": "ng e2e",
    "i18n": "node ./scripts/generatelocales",
    "ngcc:prepare": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "postinstall": "node ./scripts/locale.js",
    "build:prod": "npm run i18n && ng build --prod",
    "build:prod:stats": "npm run i18n && ng build --prod --stats-json",
    "analyzer": "webpack-bundle-analyzer dist/seed/stats-es2015.json",
    "bundle-report": "npm run build:prod:stats && npm run analyzer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.5",
    "@angular/cdk": "^10.1.0",
    "@angular/common": "~10.0.5",
    "@angular/compiler": "10.0.5",
    "@angular/core": "~10.0.5",
    "@angular/forms": "~10.0.5",
    "@angular/material": "^10.1.0",
    "@angular/material-moment-adapter": "^10.1.0",
    "@angular/platform-browser": "~10.0.5",
    "@angular/platform-browser-dynamic": "~10.0.5",
    "@angular/router": "~10.0.5",
    "@angular/service-worker": "^10.0.5",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "classlist.js": "^1.1.20150312",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "moment": "^2.27.0",
    "ngx-moment": "^5.0.0",
    "ngx-quicklink": "^0.2.3",
    "ngx-webstorage": "^6.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "^10.0.4",
    "@angular/compiler-cli": "10.0.5",
    "@angular/language-service": "~10.0.5",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.0.24",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "mergejson": "^1.1.2",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
