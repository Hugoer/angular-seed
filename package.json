{
  "name": "work-log",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run i18n && ng serve --proxy-config src/proxy.conf.json --open",
    "start:prod": "npm run i18n && ng serve --proxy-config src/proxy.conf.json --open --prod",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "gulp i18n",
    "ngcc:prepare": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "postinstall": "node locale.js && npm run ngcc:prepare",
    "build:prod": "npm run i18n && ng build --prod",
    "build:prod:stats": "npm run i18n && ng build --prod --stats-json",
    "bundle-report": "npm run build:prod:stats && webpack-bundle-analyzer dist/seed/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.2",
    "@angular/cdk": "^9.0.1",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/forms": "~9.0.2",
    "@angular/material": "^9.0.1",
    "@angular/material-moment-adapter": "^9.0.1",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "@angular/service-worker": "^9.0.2",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@ngx-translate/core": "^12.1.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "classlist.js": "^1.1.20150312",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "ngx-moment": "^3.5.0",
    "ngx-quicklink": "^0.2.0",
    "ngx-webstorage": "^5.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "^9.0.3",
    "@angular/compiler-cli": "9.0.2",
    "@angular/language-service": "~9.0.2",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "gulp": "^4.0.2",
    "gulp-merge-json": "^1.3.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
