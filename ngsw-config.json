{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/favicon.png",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/vendor/*.js",
          "/assets/fonts/*.woff2",
          "/assets/fonts/*.css",
          "/main-es2018.*.js",
          "/polyfills-es2018.*.js",
          "/runtime-es2018.*.js",
          "/common-es2018.*.js",
          "/i18n/*"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/vendor/*.*",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)",
          "./src/**/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)",
          "./src/i18n/*.json",
          "/*.js"
        ]
      }
    }
  ]
}
